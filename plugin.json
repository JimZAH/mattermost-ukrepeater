{
    "id": "com.mattermost.server-ukrepeater",
    "name": "UKRepeater",
    "server": {
        "executable": "plugin.exe"
    },
    "settings_schema": {
        "header": "Configure ukrepeater plugin",
        "footer": "Footer: For more information, speak to M0ZAH",
        "settings": [
            {
                "key": "api",
                "display_name": "Set API URL",
                "type": "text",
                "help_text": "The endpoint",
